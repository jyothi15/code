
#include <cmath>
#include <iostream>

using namespace std;


int main(){
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++){
        int n,i,sum = 0;
        cin >> n;
        for(i=3; i<n; i++) {
            if(i%              5 == 0){
                sum = sum + i ;
            }
            else if (i%3 == 0) {
                sum =sum + i;
                
            }
        }
        cout << sum ;
        cout << "\n";
    }
    return 0;
}
